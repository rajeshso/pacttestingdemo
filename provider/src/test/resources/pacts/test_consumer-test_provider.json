{
    "provider": {
        "name": "test_provider"
    },
    "consumer": {
        "name": "test_consumer"
    },
    "interactions": [
        {
            "description": "a GET REQUEST for a greeting",
            "request": {
                "method": "GET",
                "path": "/greet/John"
            },
